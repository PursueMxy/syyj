<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content">


    <com.zhkj.syyj.CustView.CircleImageView
        android:layout_margin="@dimen/dp_15"
        android:id="@+id/shop_right_img"
        android:src="@mipmap/spyj_logo"
        android:layout_width="@dimen/dp_62"
        android:layout_height="@dimen/dp_62" />
    <TextView
        android:layout_alignLeft="@+id/shop_right_img"
        android:layout_alignRight="@+id/shop_right_img"
        android:layout_below="@+id/shop_right_img"
        android:id="@+id/right_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:text="哈哈哈"
        android:textColor="@color/text_black"
        android:textSize="@dimen/sp_12" />
</RelativeLayout>
