<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <TextView
        android:id="@+id/list_integral_detail_tv_type"
        android:layout_marginLeft="@dimen/dp_11"
        android:layout_marginTop="@dimen/dp_20"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="商城赠送"
        android:textColor="@color/text_black"
        android:textSize="@dimen/sp_14"
        />
    <TextView
        android:id="@+id/list_integral_detail_tv_time"
        android:layout_marginBottom="@dimen/dp_9"
        android:layout_marginLeft="@dimen/dp_11"
        android:layout_marginTop="@dimen/dp_10"
        android:layout_below="@id/list_integral_detail_tv_type"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="2018-12-01 19:18:00"
        android:textColor="@color/text_949397"
        android:textSize="@dimen/sp_12"
        />

    <TextView
        android:id="@+id/list_integral_detail_tv_number"
        android:layout_marginRight="@dimen/dp_10"
        android:layout_marginTop="@dimen/dp_33"
        android:layout_alignParentRight="true"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="+300"
        android:textColor="@color/text_black"
        android:textSize="@dimen/sp_14"
        />
</RelativeLayout>
